<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |login_test| %>
      <tr>
        <td><%= link_to 'Show', login_test %></td>
        <td><%= link_to 'Edit', edit_login_test_path(login_test) %></td>
        <td><%= link_to 'Destroy', login_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Login test', new_login_test_path %>
